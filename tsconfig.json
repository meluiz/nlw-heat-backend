{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@app/*": [ "app/*" ],
      "@prismajs/*": [ "prismajs/*" ],
      "@service/*": [ "services/*" ],
      "@start/*": [ "start/*" ]
    }
  }
}
